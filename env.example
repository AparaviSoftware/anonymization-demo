# Aparavi API Configuration
# Copy this file to .env and fill in your actual credentials

# Your API key from https://core.aparavi.com/usage/
APARAVI_API_KEY=your-api-key-here

# API Base URL
# Production: https://eaas.aparavi.com/
# Development: https://eaas-dev.aparavi.com/
APARAVI_BASE_URL=https://eaas.aparavi.com/

# Optional: Enable debug logging
APARAVI_DEBUG=false
